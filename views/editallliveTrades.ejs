<script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
<!-- <div class="d-flex justify-content-center align-items-center">
				<div class="uname">Web developer</div>
			</div> -->
			</div>
	<div>
		<a href="/account"><div class="ph">
    <img src="assetss/img/pers.svg" alt="PROFILE" /> </div></a>
	</div>
</div>

<div class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2 text-gray">Editing: Livetrade</h1>
    <div class="btn-toolbar mb-2 mb-md 0">
    </div>
  </div>
  
  <div class="col py-3">
    <div class="row">
      <div class="col">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item active"> </li>
          </ol>
        </nav>
      </div>
      <div class="col text-end fw-lighter">
        <!-- <b>Last Updated:</b> -->
        <b>LivetradeId:</b> <b style="color: white;"><%=livetrade._id%></b>
      </div>
    </div>
  
  </div>
  
  
  <form action="/editDeposit/<%=user._id%> ?_method=PUT" method="POST">   
    <div class="row form-group mb-4">
      <div class="col">
        <label for="type">Payment Method</label>
        <input type="text" class="form-control" id="type" name="type" value=" <%=livetrade.type%> " placeholder="Payment Method" required>
      </div>
  
      <div class="col">
        <label for="currencypair">Currency Pair</label>
        <input type="text" class="form-control" id="currencypair" name="currencypair" value=" <%=livetrade.currencypair%> " placeholder="Currency Pair" required>
      </div>
    </div>

    <div class="row form-group mb-4">

    <div class="col">
      <label for="lotsize">Lot Size </label>
      <input type="text" class="form-control" id="lotsize" name="lotsize" value=" <%=livetrade.lotsize%> " placeholder="Lot Size" required>
    </div>

    <div class="col">
      <label for="entryPrice">Entry Price </label>
      <input type="text" class="form-control" id="entryPrice" name="entryPrice" value=" <%=livetrade.entryPrice%> " placeholder="Entry Price" required>
    </div>

  </div>

  <div class="row form-group mb-4">
    <div class="col">
      <label for="stopLoss">Stop Loss</label>
      <input type="text" class="form-control" id="stopLoss" name="stopLoss" value="<%=livetrade.stopLoss%> " placeholder="Stop Loss" required>
    </div>

    <div class="col">
      <label for="takeProfit">Take Profit</label>
      <input type="text" class="form-control" id="takeProfit" name="takeProfit" value=" <%=livetrade.takeProfit%> " placeholder="Take Profit" required>
    </div>
  </div>

  <div class="row form-group mb-4">
    <div class="col">
      <label for="action">Action</label>
      <input type="text" class="form-control" id="action" name="action" value="<%=livetrade.action%>" placeholder="Notification" required>
    </div>

    <!-- <div class="col">
      <label for="verifiedStatus">Verification status</label>
      <input type="text" class="form-control" id="verifiedStatus" name="verifiedStatus" value=" user.verifiedStatus%> " placeholder="Verification status" required>
    </div> -->
  </div>

  
    <div class="form-group mb-4">
      <button type="submit" class="btn btn-primary">Update Livetrade</button>
  
      <button type="button" class="btn btn-danger" data-bs-toggle="modal" id="deleteButton" data-bs-target="#deleteModal">Delete Livetrade</button>
  
    </div>
  
  </form>
  
</section>
  
  
  <div class="modal fade" tabindex="-1" role="dialog" id="deleteModal">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <div class="modal-title">You are about to remove a user Livetrade record.</div>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <p>
            This will remove the User livetrade record of <b class="fw-bold"> <%=livetrade._id%> </b><br/>
            Are you sure?
          </p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          <form action="/deletelivetrade/<%=livetrade._id%> ?_method=DELETE" method="POST" class="position-relative">
            <button type="submit" class="btn btn-primary">Yes, Remove livetrade</button>
          </form>
        </div>
      </div>
    </div>
  </div>

<%- include('partials/footer')%>